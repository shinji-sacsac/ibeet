<template>
  <div>
    <p>ダウンロード書類一覧ページ</p>
    <div v-for="n in response.list" :key="n.slug">
      <nuxt-link :to="`/download/${n.topics_id}`">
        {{ n.subject }}
      </nuxt-link>
    </div>
  </div>

</template>

<script>
export default {
  async asyncData({ $axios }) {
    return {
      response: await $axios.$get('/rcms-api/3/download'),
    };
  },
};
</script>